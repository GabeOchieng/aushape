                        <xsd:group ref="generic_fields"
                                   minOccurs="0" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="raw" type="xsd:string"/>
    </xsd:complexType>

