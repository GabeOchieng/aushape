            <xsd:attribute name="serial" type="serial" use="required"/>
            <xsd:attribute name="time" type="xsd:dateTime" use="required"/>
            <xsd:attribute name="host" type="xsd:string"/>
        </xsd:complexType>
    </xsd:element>

    <!-- Log -->
